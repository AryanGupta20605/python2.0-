def minion_game(string):
    player1=0
    player2=0
    c=len(string)
    for i in range(c):
        if s[i] in 'AEIOU':
            player1+=(c)-i
        else:
            player2+=(c)-i
    if player1>player2:
        print("Kevin",player1)
    elif player2>player1:
        print("Stuart",player2)
    elif player2==player1:
        print("Draw")
    else:
        print("Draw")

if __name__ == '__main__':
    s = input()
    minion_game(s)
